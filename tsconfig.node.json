{
    "compilerOptions": {
        "composite": true,
        "skipLibCheck": true,
        "module": "ESNext",
        "moduleResolution": "bundler",
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "noFallthroughCasesInSwitch": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "noEmit": true
    },
    "include": [
        "electron.vite.config.*",
        "src/main/**/*",
        "src/shared/**/*",
        "src/preload/**/*"
    ]
}
